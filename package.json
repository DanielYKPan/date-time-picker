{
  "name": "date-time-picker-app",
  "version": "0.0.0",
  "description": "Angular Date Time Picker",
  "keywords": [
    "Angular",
    "datepicker",
    "date picker",
    "timepicker",
    "time picker"
  ],
  "author": "daniel pan <myron.yk.pan@gmail.com>",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start_1": "ng serve --port 4201",
    "build": "ng build picker",
    "test": "ng test picker",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build picker && cp README.md dist/ansyn/picker",
    "build_css": "mkdir -p dist/ansyn/picker/assets/style && node-sass --output-style=compressed projects/picker/src/sass/picker.scss > dist/ansyn/picker/assets/style/picker.min.css",
    "npm_pack": "cd dist/ansyn/picker && npm pack",
    "package": "npm run build_lib && npm run build_css && npm run npm_pack",
    "publish": "cd dist/ansyn/picker && npm publish --access public",
    "try": "git config --list > a.txt && git config --list > a.txt"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielYKPan/date-time-picker.git"
  },
  "dependencies": {
    "@angular/animations": "^7.2.2",
    "@angular/common": "^7.2.2",
    "@angular/compiler": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/forms": "^7.2.2",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/router": "^7.2.2",
    "core-js": "^2.5.4",
    "moment": "^2.21.0",
    "moment-timezone": "^0.5.25",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular-devkit/build-ng-packagr": "^0.12.3",
    "@angular/cdk": "^7.3.0",
    "@angular/cli": "~7.2.3",
    "@angular/compiler-cli": "^7.2.10",
    "@angular/language-service": "^7.2.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.7.1",
    "node-sass": "^4.12.0",
    "protractor": "^5.4.1",
    "ts-node": "~5.0.1",
    "tsickle": "^0.34.0",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~3.2.4"
  }
}
